		"lucide-react": "^0.545.0",
		"next": "15.5.5",
		"next-themes": "^0.4.6",
		"postgres": "^3.4.7",
		"react": "19.1.0",
		"react-day-picker": "^9.11.1",
		"react-dom": "19.1.0",
		"react-hook-form": "^7.65.0",
